{
  "name": "startrail",
  "version": "0.1.0",
  "description": "Block Cache for js-ipfs",
  "main": "src/index.js",
  "scripts": {
    "style": "prettier --config .prettierrc --write '**/*'",
    "test": "mocha --recursive --exit",
    "test:ci": "npm run style && npm test",
    "test:debug": "mocha debug test/**"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "async": "^2.6.1",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^7.5.0",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "mocha": "^6.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "sinon": "^6.3.5"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnytiago/startrail.git"
  },
  "keywords": [
    "libp2p",
    "startrail"
  ],
  "bugs": {
    "url": "https://github.com/jonhytiago/startrail/issues"
  },
  "homepage": "https://github.com/jonhytiago/startrail",
  "author": "Jo√£o Tiago <joaosao10@gmail.com>",
  "license": "MIT"
}
